/* pages/level/level.wxss */
.table {

border: 1px solid #dadada;
border-right: 0;
border-bottom: 0;
width: 98%;
margin:2px;
}

.tr {
width: 100%;
display: flex;
justify-content: space-between;
}

.th, .td {
padding: 10px;
border-bottom: 1px solid #dadada;
border-right: 1px solid #dadada;
text-align: center;
width: 100%;
}

.th {
font-weight: 400;
background-color: #dadada;
}